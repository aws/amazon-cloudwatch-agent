{
  "agent": {
    "metrics_collection_interval": 60,
    "run_as_user": "root",
    "debug": true
  },
  "logs": {
    "metrics_collected": {
      "emf": {}
    }
  },
  "metrics": {
    "namespace": "ECSMetadataTest",
    "append_dimensions": {
      "InstanceId": "${aws:InstanceId}"
    },
    "metrics_collected": {
      "disk": {
        "measurement": [
          "mem_used_percent"
        ],
        "metrics_collection_interval": 60,
        "resources": [
          "*"
        ],
        "append_dimensions": {
          "khanh-ecs-fargate-test": "khanh-ecs-fargate-test"
        }
      }
    }
  }
}