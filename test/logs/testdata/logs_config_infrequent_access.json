{
  "agent": {
    "run_as_user": "root",
    "debug": true
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/tmp/test.log",
            "log_group_name": "{instance_id}-infrequent_access",
            "log_stream_name": "{instance_id}",
            "timezone": "UTC",
            "log_group_class": "Infrequent_access"
          }
        ]
      }
    }
  }
}
