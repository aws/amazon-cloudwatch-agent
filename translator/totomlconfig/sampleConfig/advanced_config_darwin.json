{
    "metrics": {
      "append_dimensions": {
        "AutoScalingGroupName": "${aws:AutoScalingGroupName}",
        "ImageId": "${aws:ImageId}",
        "InstanceId": "${aws:InstanceId}",
        "InstanceType": "${aws:InstanceType}"
      },
      "metrics_collected": {
        "cpu": {
          "measurement": [
            "cpu_usage_idle",
            "cpu_usage_iowait",
            "cpu_usage_user",
            "cpu_usage_system"
          ],
          "totalcpu": false
        },
        "disk": {
          "resources": [
            "*"
          ],
          "measurement": [
            "used_percent",
            "inodes_free"
          ]
        },
        "diskio": {
          "resources": [
            "*"
          ],
          "measurement": [
            "io_time",
            "write_bytes",
            "read_bytes",
            "writes",
            "reads"
          ]
        },
        "mem": {
          "measurement": [
            "mem_used_percent"
          ]
        },
        "netstat": {
          "measurement": [
            "tcp_established",
            "tcp_time_wait"
          ]
        },
        "swap": {
          "measurement": [
            "swap_used_percent"
          ]
        }
      }
    }
  }
  