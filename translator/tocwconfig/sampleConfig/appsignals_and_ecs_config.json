{
  "agent": {
    "region": "us-east-1"
  },
  "logs": {
    "metrics_collected": {
      "application_signals": {
        "tls": {
          "cert_file": "path/to/cert.crt",
          "key_file": "path/to/key.key"
        },
        "limiter": {
          "log_dropped_metrics": true,
          "rotation_interval": "10m"
        }
      }
    },
    "force_flush_interval": 5,
    "endpoint_override":"https://fake_endpoint"
  },
  "traces": {
    "traces_collected": {
      "application_signals": {}
    }
  }
}