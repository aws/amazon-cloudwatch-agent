{
  "agent": {
    "logfile": "/opt/tmp/a.log"
  },
  "logs": {
    "concurrency": 1,
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/opt/tmp/a.log",
            "log_group_name": "amazon-cloudwatch-agent.log",
            "timestamp_format": "%H:%M:%S %y %b %d"
          }
        ]
      }
    }
  }
}