{
  "agent": {
    "credentials": {
      "role_arn": "global_role_arn_value"
    },
    "internal": true,
    "logfile": "c:\\ProgramData\\Amazon\\AmazonCloudWatchAgent\\Logs\\amazon-cloudwatch-agent.log",
    "metrics_collection_interval": 60
  },
  "logs": {
    "credentials": {
      "role_arn": "log_role_arn_value_test"
    },
    "endpoint_override": "https://logs-fips.us-west-2.amazonaws.com",
    "force_flush_interval": 60,
    "log_stream_name": "LOG_STREAM_NAME",
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "c:\\ProgramData\\Amazon\\AmazonCloudWatchAgent\\Logs\\amazon-cloudwatch-agent.log",
            "log_group_name": "amazon-cloudwatch-agent.log",
            "timezone": "UTC"
          },
          {
            "file_path": "c:\\ProgramData\\Amazon\\AmazonCloudWatchAgent\\Logs\\test.log",
            "log_group_name": "test.log",
            "timezone": "UTC"
          },
          {
            "file_path": "c:\\ProgramData\\Amazon\\AmazonCloudWatchAgent\\Logs\\test.log"
          }
        ]
      },
      "windows_events": {
        "collect_list": [
          {
            "event_format": "xml",
            "event_levels": [
              "INFORMATION",
              "ERROR"
            ],
            "event_name": "System",
            "log_group_name": "System",
            "log_stream_name": "System"
          },
          {
            "event_format": "xml",
            "event_levels": [
              "CRITICAL",
              "VERBOSE"
            ],
            "event_name": "Application",
            "log_group_name": "Application",
            "log_stream_name": "Application"
          },
          {
            "event_format": "xml",
            "event_levels": [
              "INFORMATION"
            ],
            "event_name": "Application",
            "log_group_name": "Application",
            "log_stream_name": "Application"
          }
        ]
      }
    }
  },
  "metrics": {
    "aggregation_dimensions": [
      [
        "ImageId"
      ],
      [
        "InstanceId",
        "InstanceType"
      ],
      [
        "d1"
      ],
      []
    ],
    "append_dimensions": {
      "AutoScalingGroupName": "${aws:AutoScalingGroupName}",
      "ImageId": "${aws:ImageId}",
      "InstanceId": "${aws:InstanceId}",
      "InstanceType": "${aws:InstanceType}"
    },
    "credentials": {
      "role_arn": "metrics_role_arn_value_test"
    },
    "endpoint_override": "https://monitoring-fips.us-west-2.amazonaws.com",
    "force_flush_interval": 60,
    "metrics_collected": {
      "LogicalDisk": {
        "measurement": [
          {
            "name": "% Idle Time",
            "unit": "PERCENT"
          },
          {
            "name": "% Disk Read Time",
            "rename": "DISK_READ"
          },
          "% Disk Write Time",
          "% User Time"
        ],
        "resources": [
          "*"
        ]
      },
      "Memory": {
        "append_dimensions": {
          "d3": "win_bo"
        },
        "measurement": [
          "Available Bytes",
          "Cache Faults/sec",
          "Page Faults/sec",
          "Pages/sec"
        ],
        "metrics_collection_interval": 5
      },
      "Network Interface": {
        "append_dimensions": {
          "d3": "win_bo"
        },
        "measurement": [
          "Bytes Received/sec",
          "Bytes Sent/sec",
          "Packets Received/sec",
          "Packets Sent/sec"
        ],
        "metrics_collection_interval": 5,
        "resources": [
          "*"
        ]
      },
      "Processor": {
        "append_dimensions": {
          "d1": "win_foo",
          "d2": "win_bar"
        },
        "measurement": [
          {
            "name": "% Idle Time",
            "rename": "CPU_IDLE",
            "unit": "PERCENT"
          },
          "% Interrupt Time",
          "% User Time",
          "% Processor Time"
        ],
        "resources": [
          "*"
        ]
      },
      "System": {
        "append_dimensions": {
          "d1": "win_foo",
          "d2": "win_bar"
        },
        "measurement": [
          "Context Switches/sec",
          "System Calls/sec",
          "Processor Queue Length"
        ]
      },
      "customizedObjectName": {
        "customizedCounterName": [
          "metric1",
          "metric2"
        ],
        "measurement": [
          "customizedCounter1"
        ],
        "metrics_collection_interval": 60,
        "resources": [
          "customizedInstances"
        ]
      },
      "statsd": {
        "metric_separator": "_",
        "metrics_aggregation_interval": 60,
        "metrics_collection_interval": 10,
        "service_address": ":8125"
      }
    }
  }
}