{
  "agent": {
    "logfile": "c:\\ProgramData\\Amazon\\AmazonCloudWatchAgent\\Logs\\amazon-cloudwatch-agent.log"
  },
  "metrics": {
    "append_dimensions": {
      "InstanceId": "${aws:InstanceId}"
    },
    "metrics_collected": {
      "procstat": [
        {
          "exe": "KAVFS",
          "measurement": [
            "pid_count"
          ]
        },
        {
          "exe": "W3SVC",
          "measurement": [
            "pid_count"
          ]
        },
        {
          "exe": "IISADMIN",
          "measurement": [
            "pid_count"
          ]
        }
      ]
    }
  }
}