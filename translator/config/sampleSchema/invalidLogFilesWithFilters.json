{
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/opt/aws/amazon-cloudwatch-agent/logs/amazon-cloudwatch-agent.log",
            "log_group_name": "amazon-cloudwatch-agent.log",
            "log_stream_name": "amazon-cloudwatch-agent.log",
            "multi_line_start_pattern": "{timestampe_format}",
            "timestamp_format":"%H:%M:%S %y %b %d",
            "timezone": "UTC",
            "filters": [
              {
                "type": "match",
                "expression": "is an incorrect value for 'type'"
              },
              {
                "type": "exclude",
                "expression": "(TRACE|DEBUG)"
              }
            ]
          },
          {
            "file_path": "/opt/aws/amazon-cloudwatch-agent/logs/test.log",
            "log_group_name": "test.log",
            "log_stream_name": "test.log",
            "timezone": "Local",
            "filters": [
              {
                "type": "exclude",
                "expression": "Foo: ([1-5]\\d\\d)",
                "foo": "bar"
              }
            ]
          },
          {
            "file_path": "/opt/aws/amazon-cloudwatch-agent/logs/*",
            "blacklist": "agent.log*|env.log|profiler.log|\\.\\d$",
            "publish_multi_logs": true,
            "timezone": "UTC"
          }
        ]
      }
    },
    "log_stream_name": "LOG_STREAM_NAME"
  }
}