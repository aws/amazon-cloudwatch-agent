{
  "logs": {
    "logs_collected": {
      "journald": {
        "collect_list": [
          {
            "log_group_name": "system-logs",
            "log_stream_name": "{instance_id}",
            "units": ["systemd"],
            "filters": [
              {
                "type": "invalid_type",
                "expression": ".*debug.*"
              }
            ]
          }
        ]
      }
    },
    "log_stream_name": "LOG_STREAM_NAME"
  }
}